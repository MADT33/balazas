<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   xmlns:mvc="sap.ui.core.mvc"> 

   <Dialog
      id="Mosico"
      title="Ifix Enviar Mosico" >

       <Table id="TableMosico" fixedLayout="true" growing="true"  growingThreshold="7" items="{IfixMosicoModel>/}" >
		<columns>
          <Column>
				<Label text="{i18n>Num_Transporte}"/>
			</Column>
         
			<Column>
				<Label text="{i18n>Conductor}"/>
			</Column>

			<Column>
				<Label text="{i18n>Codip}"/>
			</Column>

			<Column>
				<Label text="{i18n>Mensaje}"/>
			</Column>

			<Column>
				<Label text="{i18n>Patente}"/>
			</Column>

			<Column>
				<Label text="{i18n>Pico}"/>
			</Column>

			<Column>
				<Label text="{i18n>Producto}"/>
			</Column>

			<Column>
				<Label text="{i18n>Volumen}"/>
			</Column>

			
			
		</columns>
		<items>
			<ColumnListItem type="Active">
				<cells>
					<Text text="{IfixMosicoModel>Num_Transporte}"/>
					<Text text="{IfixMosicoModel>Conductor}"/>
					<Text text="{IfixMosicoModel>Codip}"/>
					<Text text="{IfixMosicoModel>Mensaje}"/>
					<Text text="{IfixMosicoModel>Patente}"/>
					<Text text="{IfixMosicoModel>Pico}"/>
					<Text text="{IfixMosicoModel>Producto}"/>
               <Text text="{IfixMosicoModel>Volumen}"/>
				
				</cells>
			</ColumnListItem>
		</items>
	</Table>

      <HBox id="hb11" alignItems="End" width="auto" justifyContent="End" height="auto" direction="Row" >				
	
	<Button id="FBTN11" text="Cerrar"  type="Emphasized" press="CerrarDialogo" class="sapUiTinyMarginEnd"></Button>

	</HBox>




         </Dialog>
</core:FragmentDefinition>